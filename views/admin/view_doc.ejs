<%- include('../_layouts/admin_header'); %>
	
	<h1 class="page-title" style="font-family: Times New Roman;"><%= name %></h1>
	<a href="/admin/tables" class="btn btn-primary" style="font-size: 20px; font-family: Times New Roman;">Back to tables</a>
	<br><br>
	<table class="table table-dark table-hover" style="font-size: 20px; font-family: Times New Roman;">
	<thead>
	<tr>
	<th><%=field%></th>
	<th>Edit</th>
	<th>Delete</th>
	</tr>
	</thead>
	<tbody>
	<% for(var i=0;i<docs.length;i++){ %>
		<tr>
		<td><%=docs[i]%></td>
		<td><a href="/admin/tables/edit-doc/<%=tableid%>/<%=doc_id[i]%>"  style="color: powderblue;">Edit</a></td>
		<td><a class="confirmDeletion" href="/admin/tables/delete/<%=tableid%>/<%=doc_id[i]%>"  style="color: powderblue;">Delete</a></td>
		</tr>
	<% } %>
	</tbody>
	</table>
<%- include('../_layouts/admin_footer') %>